<section class="home">
  <section class="page_body">
    <%= render 'filter' %>
    <section class="slog_list">
      <%= will_paginate @slogs %>
        <% @slogs.each do |slog| %>
            <%= render "slog_item", :slog => slog %>
        <% end %>
      <%= will_paginate @slogs, class: 'pagination_bot' %>
    </section>
  </section>
  <%= render "shared/sidebar" %>
</section>
